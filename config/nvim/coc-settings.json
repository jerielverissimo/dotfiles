{
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSaveFiletypes": ["rust", "dart", "c", "cpp", "go"],
  "diagnostic.displayByAle": true,
  "rust-analyzer.cargo.allFeatures": true,
  "rust-analyzer.updates.channel": "nightly",
  "flutter.openDevLogSplitCommand": "botright 10split",
  "list.source.files.excludePatterns": ["**/node_modules/*", "**/.git/*"],
  "list.source.mru.excludePatterns": ["**/node_modules/*", "**/.git/*"],
  "list.source.grep.excludePatterns": [
    "**/node_modules/*",
    "**/.git/*"
  ],
  "emmet.includeLanguages": {"vue-html": "html", "javascript": "javascriptreact"},
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    },
    "haskell": {
      "command": "hie-wrapper",
      "rootPatterns": [
        ".stack.yaml",
        "cabal.config",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ],
      "initializationOptions": {
        "languageServerHaskell": {
        }
      }
    }
  }
}
